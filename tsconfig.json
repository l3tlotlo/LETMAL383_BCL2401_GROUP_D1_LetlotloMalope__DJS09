{
  "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": true,
    "module": "es2015",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  }
} 
